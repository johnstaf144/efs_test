     version="v2"
     version-type="context"
     transports="https">
   <resource methods="POST"
             url-mapping="/AbortWithResponse"
             faultSequence="fault">
      <inSequence>
         <property name="api.ut.backendRequestTime"
                   expression="get-property('SYSTEM_TIME')"/>
         <filter source="$ctx:AM_KEY_TYPE" regex="PRODUCTION">
            <then>
               <send>
                  <endpoint name="admin--etax_APIproductionEndpoint_0">
                     <http uri-template="https://etax-service-eip.apps.ose.cp.tio.systems/v2/Tax"/>
                     <property name="ENDPOINT_ADDRESS"
                               value="https://etax-service-eip.apps.ose.cp.tio.systems/v2/Tax"/>
                  </endpoint>
               </send>
            </then>
            <else>
               <send>
                  <endpoint name="admin--etax_APIsandboxEndpoint_0">
                     <http uri-template="none"/>
                     <property name="ENDPOINT_ADDRESS" value="none"/>
                  </endpoint>
               </send>
            </else>
         </filter>
      </inSequence>
      <outSequence>
         <class name="org.wso2.carbon.apimgt.gateway.handlers.analytics.APIMgtResponseHandler"/>
         <send/>
      </outSequence>
   </resource>
