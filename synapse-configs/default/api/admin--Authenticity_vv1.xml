   <resource methods="POST"
             url-mapping="/markProductShippedPS"
             faultSequence="fault">
      <inSequence>
         <property name="api.ut.backendRequestTime"
                   expression="get-property('SYSTEM_TIME')"/>
         <filter source="$ctx:AM_KEY_TYPE" regex="PRODUCTION">
            <then>
               <send>
                  <endpoint name="admin--Authenticity_APIproductionEndpoint_0">
                     <http uri-template="https://authenticity-service-eip.apps.ose.cp.tio.systems/cxf/authenticity">
                        <timeout>
                           <duration>120000</duration>
                           <responseAction>fault</responseAction>
                        </timeout>
                     </http>
                     <property name="ENDPOINT_ADDRESS"
                               value="https://authenticity-service-eip.apps.ose.cp.tio.systems/cxf/authenticity"/>
                  </endpoint>
               </send>
            </then>
            <else>
               <sequence key="_sandbox_key_error_"/>
            </else>
         </filter>
      </inSequence>
      <outSequence>
         <class name="org.wso2.carbon.apimgt.gateway.handlers.analytics.APIMgtResponseHandler"/>
         <send/>
      </outSequence>
   </resource>
